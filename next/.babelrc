{
  "presets": [
    [
      "next/babel",
      {
        "class-properties": {
          "loose": true
        },
        "preset-typescript": {
          "onlyRemoveTypeImports": true
        }
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "babel-plugin-parameter-decorator",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@@types": "./src/types"
      }
    }],
  ]
}