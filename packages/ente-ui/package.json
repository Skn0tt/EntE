{
  "name": "ente-ui",
  "repository": "https://gitlab.com/skn0tt/EntE/master/packages/ente-ui",
  "version": "0.17.2",
  "private": true,
  "scripts": {
    "start": "yarn parcel src/index.html",
    "build": "touch ./src/sw.js && yarn parcel build src/index.html --public-url / -o index && mv dist/report.html report.html && yarn serviceworker && size-limit",
    "watch": "yarn parcel watch src/index.html --public-url / -o index",
    "docker:replace": "docker exec ente_ui_1 /usr/local/bin/replace",
    "serviceworker": "yarn workbox generateSW",
    "snyk": "snyk test",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint": "tslint --project ."
  },
  "dependencies": {
    "@date-io/date-fns": "^0.0.2",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@sentry/browser": "^4.3.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.24",
    "ente-types": "^0.17.2",
    "immutable": "^4.0.0-rc.12",
    "luxon": "^1.4.5",
    "material-ui-pickers": "^2.0.4",
    "monet": "^0.9.0",
    "mui-datatables": "^2.0.0-beta-42",
    "mui-downshift": "^1.2.1",
    "papaparse": "^4.6.1",
    "query-string": "^6.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-dropzone": "^4.2.7",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-vis": "^1.11.6",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-persist": "^4",
    "redux-persist-transform-immutable": "^4.1",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3",
    "typeface-roboto": "^0.0.45"
  },
  "devDependencies": {
    "@babel/runtime": "^7.1.2",
    "@types/jest": "^21.1.9",
    "@types/js-cookie": "^2.1.0",
    "@types/node": "^8.5.2",
    "@types/papaparse": "^4.5.5",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-easy-chart": "^0.1.4",
    "@types/react-redux": "^6.0.11",
    "@types/react-router": "^4.3.1",
    "@types/react-router-dom": "^4.2.3",
    "@types/redux-actions": "^2.3.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prop-types": "^15.6.2",
    "redux-devtools-extension": "^2.13.5",
    "size-limit": "^0.21.0",
    "source-map-explorer": "^1.5.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.8.0",
